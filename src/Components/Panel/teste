<FormControl >
                     {setSpecialities.map(value => (
                            <Chip key={value} label={value}/>
                        ))}
                    </FormControl>


                    <FormControl>
                    <InputLabel htmlFor="select-multiple-checkbox">Especialidades</InputLabel>
                        <Select
                            multiple
                            value={specialities.name}
                            onChange={e => (dispatch(handleChangeFilter(e)))}
                            input={<Input id="select-multiple-checkbox" />}
                            renderValue={selected => selected.join(', ')}
                            MenuProps={MenuProps}
                        >
                        {specialities.map(el => (
                            <MenuItem key={el.key} value={el.name}>
                                <Checkbox checked={setSpecialities.indexOf(el.name) > -1} />
                                <ListItemText primary={el.name} />
                            </MenuItem>
                        ))}
                        </Select>
                 </FormControl>

                 const ITEM_HEIGHT = 48;
const ITEM_PADDING_TOP = 8;
const MenuProps = {
  PaperProps: {
    style: {
      maxHeight: ITEM_HEIGHT * 4.5 + ITEM_PADDING_TOP,
      width: 250,
    },
  },
};


const handleChangeFilter = (e) =>({
    type: SET_SPECIALITIES,
    specialities: e.value
})
